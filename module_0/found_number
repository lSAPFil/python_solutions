def found_number(number):
    "Function for find the number of user"
    one_part_of_range = 0
    two_part_of_range = 20
    three_part_of_range = 40
    four_part_of_range = 60
    five_part_of_range = 80
    step = 0
    i = 0
    while i != number:
        one_part_of_range += 1
        two_part_of_range += 1
        three_part_of_range += 1
        four_part_of_range += 1
        five_part_of_range += 1
        step += 1
        if (one_part_of_range == int(number)
                or two_part_of_range == int(number)
                or three_part_of_range == int(number)
                or four_part_of_range == int(number)
                or five_part_of_range == int(number)):
            i = number
    print("This program was able to find your number in {step} step(s). Your number is { number}")

number = input("Enter your number ")
found_number(number)
